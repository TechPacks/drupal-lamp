    "recipe[git]",
    "recipe[mysql::server]",
    "recipe[drupal::ssh]",
    "recipe[drupal::apache]",
    "recipe[drupal::php]",
    "recipe[drupal::drush]",
    "recipe[drupal::mysql]",
    "recipe[drupal]"
  ]
}
